<p id="notice"><%= notice %></p>

<h1>Wines</h1>

<table class="table table-dark table-sm">
  <thead>
    <tr>
      <th>Name</th>
      <th>Oenologists</th>
      <th>Score</th>
      <th>strain</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wines.each do |wine| %>
      <tr>
        <td><%= wine.name %></td>
        <td><%= wine.get_oenologist%></td>
        <td><%= wine.score %></td>
        <td><%= wine.strainnames_and_percentages %></td>
        <td><%= link_to 'Show', wine %></td>
        <td><%= link_to 'Edit', edit_wine_path(wine) %></td>
        <td><%= link_to 'Destroy', wine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wine', new_wine_path %>
